{
  "name": "e2e-ui-test",
  "version": "1.0.0",
  "description": "End to End Test for Live Chat",
  "main": "index.js",
  "scripts": {
    "cy": "./node_modules/.bin/cypress",
    "cy:open": "npm run cy open",
    "test": "npm run cy run -- -q --headed",
    "cy:headless": "npm run cy run -- -q",
    "cy:home": "npm run cy run -- --spec cypress/integration/examples/HomePageTest.js",
    "cy:login": "npm run cy run -- --spec cypress/integration/examples/LogInPageTest.js",
    "cy:signup": "npm run cy run -- --spec cypress/integration/examples/SignUpTest.js",
    "cy:events": "npm run cy run -- --spec cypress/integration/examples/EventPageTestjs",
    "ci": "npm run cy run -- -q"
  },
  "author": "Ankit A",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^5.5.0"
  }
}
